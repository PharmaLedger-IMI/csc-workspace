let t=new class{setIdentity(t){this.identity=t}setSSAppPage(t){this.ssAppPage=t}getSSAppPage(){return this.ssAppPage?this.ssAppPage:this.identity}listenForSSAppHistoryChanges(){window.document.addEventListener("ssapp-history-changed",t=>{let e=t.detail;e.ssappPageUrl&&this.setSSAppPage(this.identity+e.ssappPageUrl);let n="~"+this.getSSAppPage();e.childRoute&&(n+=e.childRoute);let s=e.ssappPageUrl?e.ssappPageUrl:e.currentPageTitle;console.log(n,s),this.notifyParentForChanges({currentPageTitle:s,childRoute:n})})}notifyParentForChanges(t){(()=>{try{return window.self!==window.parent}catch(t){return!1}})()&&window.parent.document.dispatchEvent(new CustomEvent("ssapp-history-changed",{detail:t}))}};const e={getInstance:function(){return t},notifyParentForChanges:t.notifyParentForChanges};export{e as N}